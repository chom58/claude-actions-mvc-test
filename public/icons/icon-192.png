<?xml version="1.0" encoding="UTF-8"?>
<svg width="192" height="192" viewBox="0 0 192 192" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="192" height="192" fill="#0a0a0a" rx="32"/>
  
  <!-- Neon gradient background -->
  <defs>
    <linearGradient id="neonGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF1493;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#00FFFF;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#FF00FF;stop-opacity:0.8" />
    </linearGradient>
    
    <linearGradient id="textGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF1493" />
      <stop offset="100%" style="stop-color:#00FFFF" />
    </linearGradient>
    
    <!-- Glow filter -->
    <filter id="glow">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background pattern -->
  <circle cx="48" cy="48" r="20" fill="url(#neonGradient)" opacity="0.3"/>
  <circle cx="144" cy="48" r="15" fill="#FF00FF" opacity="0.4"/>
  <circle cx="48" cy="144" r="12" fill="#00FFFF" opacity="0.5"/>
  <circle cx="144" cy="144" r="18" fill="#00FF00" opacity="0.3"/>
  
  <!-- Main logo text -->
  <text x="96" y="120" text-anchor="middle" 
        font-family="Arial, sans-serif" 
        font-size="48" 
        font-weight="900" 
        fill="url(#textGradient)"
        filter="url(#glow)">HCC</text>
  
  <!-- Subtitle -->
  <text x="96" y="145" text-anchor="middle" 
        font-family="Arial, sans-serif" 
        font-size="12" 
        font-weight="400" 
        fill="#ffffff" 
        opacity="0.8">HARAJUKU</text>
  
  <!-- Border highlight -->
  <rect width="192" height="192" fill="none" stroke="url(#neonGradient)" stroke-width="2" rx="32" opacity="0.6"/>
</svg>