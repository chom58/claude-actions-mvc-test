<?xml version="1.0" encoding="UTF-8"?>
<svg width="128" height="128" viewBox="0 0 128 128" xmlns="http://www.w3.org/2000/svg">
  <rect width="128" height="128" fill="#0a0a0a" rx="21"/>
  <defs>
    <linearGradient id="neonGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF1493;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#00FFFF;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#FF00FF;stop-opacity:0.8" />
    </linearGradient>
    <linearGradient id="textGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF1493" />
      <stop offset="100%" style="stop-color:#00FFFF" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  <circle cx="32" cy="32" r="13" fill="url(#neonGradient)" opacity="0.3"/>
  <circle cx="96" cy="32" r="10" fill="#FF00FF" opacity="0.4"/>
  <text x="64" y="80" text-anchor="middle" font-family="Arial, sans-serif" font-size="32" font-weight="900" fill="url(#textGradient)" filter="url(#glow)">HCC</text>
  <text x="64" y="97" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="400" fill="#ffffff" opacity="0.8">HARAJUKU</text>
  <rect width="128" height="128" fill="none" stroke="url(#neonGradient)" stroke-width="2" rx="21" opacity="0.6"/>
</svg>